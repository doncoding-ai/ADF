{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-traning-don"
		},
		"ls_ablob_covidreportingboom_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingboom'"
		},
		"ls_adls_covidreportingloom_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingloom'"
		},
		"ls_adls_covidreportingloom_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingloom.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f4f2d56a-35c6-453f-a783-504ef6cbf3e1/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingboom"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-traning-don_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingboom_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingboom_connectionString')]"
					},
					"ls_adls_covidreportingloom_accountKey": {
						"value": "[parameters('ls_adls_covidreportingloom_accountKey')]"
					},
					"ls_adls_covidreportingloom_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingloom_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}